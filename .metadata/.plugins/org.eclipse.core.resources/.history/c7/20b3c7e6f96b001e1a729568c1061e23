package org.same;

public class Loop {
	public static void main(String[] args) {

		String s = "Java is Simple";
		
		String s1 = "";
		
		String s2 ="";
		
		String s3 = "";
		
		String [] s4  = s.split(" ");
		
		for(int i = s3[0].length()-1 ; i>=0 ; i--) {
			char c = s3[0].charAt(i);
		}
		for(int i = s3[1].length()-1 ; i>=0 ; i--) {
			char c = s3[1].charAt(i);
		}
		for(int i = s3[2].length()-1 ; i>=0 ; i--) {
			char c = s3[2].charAt(i);
		}
		
		
	
	
	
	}
}
