package org.same;

public class Amstrong {
   public static void main(String[] args) {
	    int i=0,j=0;
	    int n=a;
	    for(int a=2;a<1000;a++) {
	    	
	    }
	    while(n>0) {
	    	i=n%10;
	    	j=(i*i*i);
	    	n=n/10;
	    	
	    }
	    if (n==j) {
	    	System.out.println("Arm:"+j);
	    	
	    }
	    
	    
}
}
