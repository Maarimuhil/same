package org.selenium;

import java.util.Scanner;

public class Javaprogram {
	
	
        public static void main(String[]args){
             String s="Welcome to java";
             String s1="";
             for(int i=s.length()-1;i>=0;i--) {
            	 char c=s.charAt(i);
            	 s1=s1+c;
            	 
             }
             System.out.println(s1);
        }
}
             
               
               
            	 
            	 
            	 
            	 
            	 
            	 
            	 
            	 
            	 
            	 
            	 
            	 
            	 
             
        	
        	
        	
        	
        	
            	  
            	  
              
              
              
              
              
              
        
       
           
	
