package org.same;

public class Loop {
	public static void main(String[] args) {

		for (int a = 0; a < 10; a++) {

		int i = 0, j = 0;

		int n = a;

		while (n > 0) {
			i = n % 10;
			j = (i * i * i) + j;
			n = n / 10;
		}
		if (a == j) {
			System.out.println("Arm : " + j);

		}

		else {
			System.out.println("Not a Arm : " + j);
		}

	}

}
}
